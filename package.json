{
  "name": "dbms",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "typeorm": "ts-node ./node_modules/typeorm/cli -d src/config/database/mssql/db.config.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "start": "ts-node src/main.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "build": "tsc",
    "dbm:drop": "npm run typeorm schema:drop",
    "dbm:sync": "npm run typeorm schema:sync",
    "dbm:generate": "npm run typeorm -- migration:generate src/database/mssql/migrations/$npm_config_name",
    "dbm:run": "npm run typeorm -- migration:run",
    "dbm:revert": "npm run typeorm -- migration:revert"
  },
